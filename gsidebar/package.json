{
  "name": "sap-anywhere",
  "private": true,
  "version": "1.0.4",
  "description": "Connect SAP Anywhere to your Gmail, Contacts and Calendar, and create, edit and manage details directly from Gmail.",
  "scripts": {
    "build": "gulp --production --vendor=chrome && gulp pack --vendor=chrome",
    "build:chrome": "gulp --production --vendor=chrome && gulp pack --vendor=chrome",
    "build:firefox": "gulp --production --vendor=firefox && gulp pack --vendor=firefox",
    "build:opera": "gulp --production --vendor=opera && gulp pack --vendor=opera",
    "dev": "gulp --watch --vendor=chrome",
    "dev:chrome": "gulp --watch --vendor=chrome",
    "dev:firefox": "gulp --watch --vendor=firefox",
    "dev:opera": "gulp --watch --vendor=opera",
    "release": "gulp release",
    "dev_es5": "gulp --watch --es5 --vendor=chrome ",
    "transform": "babel ./app/scripts --out-dir ./dist/lib"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.2.0",
    "debounce": "^1.0.0",
    "del": "^2.0.2",
    "eslint": "^1.3.1",
    "eslint-loader": "^1.0.0",
    "gulp": "^3.9.0",
    "gulp-bump": "^0.3.1",
    "gulp-cache": "^0.2.10",
    "gulp-filter": "^3.0.1",
    "gulp-git": "^1.2.4",
    "gulp-if": "^1.2.5",
    "gulp-imagemin": "^2.3.0",
    "gulp-json-transform": "^0.2.0",
    "gulp-less": "^3.0.3",
    "gulp-livereload": "^3.8.0",
    "gulp-minify-css": "^1.2.1",
    "gulp-plumber": "^1.0.1",
    "gulp-sass": "^2.0.4",
    "gulp-sequence": "^0.4.1",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-tag-version": "^1.3.0",
    "gulp-util": "^3.0.5",
    "gulp-zip": "^3.0.2",
    "json-loader": "^0.5.4",
    "vinyl-named": "^1.1.0",
    "webpack": "^1.12.12",
    "webpack-stream": "^3.0.0",
    "yargs": "^3.21.0"
  },
  "dependencies": {
    "jquery": "2.2.1",
    "jquery.scrollto": "2.1.2",
    "material-ui": "0.14.4",
    "moment": "2.11.2",
    "moment-timezone": "0.5.0",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-tap-event-plugin": "0.2.2",
    "reflux": "0.3.0",
    "reflux-core": "0.3.0",
    "reflux-promise": "1.0.4",
    "require-dir": "0.3.0",
    "underscore": "1.8.3"
  }
}
